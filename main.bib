@article{Koz18,
  author={Serge Kozlukov},
  title={The method of similar operators in the study of the spectra of the adjacency matrices of graphs},
  journal={Journal of Physics: Conference Series},
  volume={973},
  number={1},
  pages={012036},
  url={http://stacks.iop.org/1742-6596/973/i=1/a=012036},
  year={2018},
  abstract={The method of similar operators [1, 2, 3] is used to investigate spectral properties of a certain class of matrices in the context of graphs [4, 5]. Specifically, we consider the adjacency matrix of an “almost-complete graph”. Then we generalize the result to allow the matrices obtained as combinations of the Kronecker products [6, 7] and the small-norm perturbations. We derive the estimates of the spectra and the eigenvectors of such matrices.},
  keywords={my,peerReviewed},
}
	
